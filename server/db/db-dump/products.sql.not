DROP TABLE IF EXISTS tea;
CREATE TABLE IF NOT EXISTS tea
(
    id    INT             PRIMARY KEY,
    brand	varchar(50)     NOT NULL,
    name	varchar(50)     NOT NULL,
    tags	varchar(50)     NOT NULL,
    image	varchar(300)    NOT NULL
);

